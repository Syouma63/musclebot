$("#follow-button_<%= @user.id %>").html("<%= j(render partial: 'users/follow-button',locals: { user: @user }) %>");
$("#follow-buttons_<%= user.id %>").html("<%= j(render partial: 'users/follow-buttons',locals: { user: user }) %>");
$("#follower-count_<%= @user.id %>").html('<%= link_to "#{ @user.followed.count }フォロワー", users_follower_path(@user) %>');
$("#followed-count_<%= current_user.id %>").html('<%= link_to "#{ current_user.follower.count }フォロー中", users_following_path(@user) %>');